

<div class="main-2">
  <div class="card active-field capital sticky">{{role}}:</div>
  {{#each results}}
    <a href="/hotel/details/{{this.username}}" class="this">
  <div class="card">
    <div class="image">
      <img
        src="{{this.image}}"
        alt=""
        srcset=""
        onerror=this.src="data:image/jpeg;base64,"
      />
    </div>
    <div class="profile-description">
        <div class="name">{{this.name}}</div>
            <div class="username">{{this.id}}</div>
            <div class="details">{{this.details}}</div>
            <div class="cost">{{this.cost}}</div>
    </div>
    
<form action="/customer/cart/{{this.id}}" method="post">
    <input type="submit" value="Add">
</form>
  </div>
</a>
  {{/each}}
</div>





