{{#each objResult}}
    <div class="border">
        <header id="site-header layer-1">
            <div class="container layer-1">
                {{!-- <section id="cart">      --}}
                        {{!-- <article class="product"> --}}
                            <div class="content">
                                <h1 >{{@key}}</h1>
                            </div>
                        {{!-- </article> --}}
                {{!-- </section> --}}
            </div>
        </header>     {{!-- timestamp --}}
        {{#each this}}
            {{!-- <h2>{{@key}}</h2>  --}}
                 {{!-- <header id="site-header head"> --}}
                    <div class="container layer-2">
                        <section id="cart">     
                                <article class="product">
                                    <header>
                                        <a class="remove">
                                            <img src="/hotel/{{this.data.h_image}}" alt="">
                                            <h3>Remove product</h3>
                                        </a>
                                    </header>
                                    <div class="content">
                                        <h1 class="danger-text" >{{this.data.h_name}}</h1>
                                        {{this.data.h_username}}
                                        {{this.data.h_phone}}
                                        {{this.data.h_address}}
                                    </div>
                                    <footer class="content">
                                        <h2 class="full-price danger-block">
                                            {{this.data.h_delivery_cost}}
                                        </h2>

                                        <form action="/delivery/order/accept/{{@key}}/{{@../key}}" method="post">
                                        <button type="submit">Accept</button>
                                        </form>
                                        
                                    </footer>
                                </article>
                        </section>
                    </div>
                {{!-- </header> --}}

                
                

                {{#each this.order_info}}

                    {{!-- {{@key}} --}}
                    {{!-- {{#ifCond @key "1"}}
                        <div class="container layer-3">
                                        <div class="content">
                                            <h1 >Requires Delivery</h1>
                                        </div>
                        </div>
                    {{else}}
                    <div class="container layer-3">
                                        <div class="content">
                                            <h1 >No Delivery Required</h1>
                                        </div>
                        </div>
                    {{/ifCond}} --}}

                        
                    <div class="container layer-3">
                                    <div class="content">
                                        <h1>{{@key}}</h1>
                                    </div>
                                    
                    </div>

                                <div class="container layer-4">

                                        {{#each orders}}

                                            <section id="cart"> 
                                                    <article class="product">
                                                        <header>
                                                                <a class="remove">
                                                                    <img src="/item/{{this.i_image}}" alt="">
                                                                    <h3>Cancel order</h3>
                                                                </a>
                                                        </header>

                                                        <div class="content">

                                                            <h1>{{this.i_name}}  {{this.available}}</h1>
                                                            {{this.i_details}}
                                                        </div>

                                                        <footer class="content">
                                                                    <span class="material-icons-outlined qt green md-18">
                                                                        shopping_bag
                                                                    </span>
                                                                    <span class="qt"><input disabled min="1" type="number" name=quantity value={{this.i_quantity}} id=""></span>
                                                                    
                                                                    
                                                                    <form action="/delivery/order/{{this.id}}/payment/completed" method="post">
                                                                        <span class="qt"><button type="submit">payment complete?</button></span>
                                                                    </form>
                                                                    <form action="/delivery/order/{{this.id}}/status/completed" method="post">
                                                                        <span class="qt"><button type="submit">order complete?</button></span>
                                                                    </form>
    
                                                            <h2 class="full-price">
                                                                {{this.total_i_cost}} {{this.order_status}}
                                                            </h2>

                                                            <h2 class="price">
                                                                {{this.cost}} {{this.payment_status}}
                                                            </h2>
                                                        </footer>
                                                    </article>
                                            </section>

                                        {{/each}}

                                    </div>
                        
                    {{/each}}
        {{/each}}
    </div>
{{/each}}